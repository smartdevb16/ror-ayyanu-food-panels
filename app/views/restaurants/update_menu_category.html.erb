<script type="text/javascript">
  <% if flash[:success].present? %>
    swal("Success", "<%= flash[:success] %>", "success")
  <% elsif flash[:error].present? %>
    swal("Error", "<%= flash[:error] %>", "error")
  <% end %>

  $("#versions_form").submit(function(){
    var time = /\b((1[0-2]|0?[1-9]):([0-5][0-9]) ([AaPp][Mm]))$/;
    var start_date = $.trim($('#datepicker').val())
    var end_date = $.trim($('#datepicker2').val())

    if($("#add_category_title").val().length >= 41 || $("#add_category_title").val().length <= 0) {
      $("#add_category_title").focus();
      swal("Warning", "Please enter valid english title", "warning");
      return false;
    } else if ($("#add_category_title_ar").val().length >= 41 || $("#add_category_title_ar").val().length <= 0 ) {
      $("#add_category_title_ar").focus();
      swal("Warning", "Please enter valid arabic title", "warning");
      return false;
    } else if($.trim($('#datepicker').val())===""){
      swal("warning", "Please Select Start Date", "warning");
      $("#datepicker").focus();
      return false;
    } else if(start_date > end_date){
      swal("warning", "Please Select Valid End date", "warning");
      $("#datepicker2").focus();
        return false;
    } else {
      return true;
    }
  });

  function delete_menu_category(no) {
    swal({
      title: "Are you sure?",
      text: "Do you want to Delete this Menu Category?",
      type: "warning",
      showCancelButton: true,
      confirmButtonColor: "#DD6B55",
      confirmButtonText: "Yes, Delete it!",
      cancelButtonText: "No, cancel plz!",
      closeOnConfirm: false,
      closeOnCancel: false
    },
    function(isConfirm){
      if (isConfirm) {
        $.ajax({
          type: "GET",
          url: "/remove/menu/category/" + no,
          dataType: "JSON",
          success:function(data) {
            if (data.code == 200){
              swal("Deleted!", "Menu category has been deleted.", "success");
              window.location.href = "<%= admin_branch_menu_items_path(encode_token(@menu_category.branch_id)) %>";
            }
          },
          error: function() {
            swal("Error", "Something went wrong", "Error")
          }
        });
      } else {
        swal("Cancelled", "No change in menu category", "error");
      }
    });
  };

  $(function() {
    var opened = false;

    var dp = $("#datepicker").datepicker({
      autoclose: true,
      todayHighlight: true,
      format: "yyyy-mm-dd",
      startDate: new Date(),
    }).on("show", function() {
    });
  });

  $(function() {
    var opened = false;

    var dp = $("#datepicker2").datepicker({
      autoclose: true,
      todayHighlight: true,
      format: "yyyy-mm-dd",
      startDate: new Date(),
    }).on("show", function() {
    });
  });
</script>