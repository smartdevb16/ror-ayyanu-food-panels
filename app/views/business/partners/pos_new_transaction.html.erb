    select_transaction();
  });
  function select_transaction(){
    // $('.transaction_selection').click(function(e){
    //   e.preventDefault();
    //   $('.transaction_selection').removeClass('selected_transaction')
    //   $(this).addClass('selected_transaction');
    //   $('#selected_menu_item').val($(this).data('transaction-id'));
    // });
    $('.qty_selection').click(function(e){
      e.preventDefault();
      e.stopPropagation();
      $('.qty_selection').removeClass('selected_qty')
      $(this).addClass('selected_qty');
      var selectedVal = $('#selected_qty').val() + $(this).data('selected-qty').toString();
      $('#selected_qty').val(selectedVal);
      $('.numberpadDisplaySelection').text(selectedVal);
      $('.quickPaynumberpadDisplaySelection').text(selectedVal);
      setTimeout(function(){
        $('.qty_selection').removeClass('selected_qty');
      }, 150)
    })
  }
</script>

