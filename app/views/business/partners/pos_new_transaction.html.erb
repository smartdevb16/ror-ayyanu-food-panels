<div class="container-fluid">
   <div class="row content">
      <div class="col-sm-12">
          <div class="well top-header">
            <div class="row">
               <div class="col-sm-12">
                  <div class="foodtop">
                      <ul>
                        <li><a href="javascript:void(0)"><i class="icon-time fa fa-calendar" aria-hidden="true"></i><span  id='current_date'>04-12-2019</span></a></li>
                        <li><a href="javascript:void(0)"><i class="icon-time fa fa-clock-o" aria-hidden="true"></i><span  id='current_time'>2:25</span></a></li>
                      </ul>
                      <h3 style="font-size: 21px;text-align: center;display: inline-block;margin-top:2%">Guest Count: <span class="displayNoofGuest"><%= @pos_check.present? ? @pos_check&.no_of_guest || 0 : @pos_table.no_of_guest ? check_guest_type(@pos_table.no_of_guest) : 0 %></span>
                      </h3>
                      <h3 style="font-size: 21px;float: right;margin-top:2%" class="numberpadDisplaySelection">
                      </h3>
                  </div>
               </div>
            </div>
            <div class="food">
               <ul>
                  <li><a class="<%= @pos_check.order_type&.name&.titleize&.upcase ? 'active' : '' %> menu-list" href="javascript:void(0)" id='foods'>Food</a></li>
                  <!-- <li><a class='menu-list' href="#news">Beverages</a></li> -->
                  <li><a class="menu-list <%= @pos_check.check_status.eql?('closed') ? 'reopen_check' : '' %>" href="javascript:void(0)" id='condiments'>Condiment's</a></li>
                  <li><a class="menu-list delivery_restriction <%= @pos_check.order_type&.name&.titleize&.upcase.eql?('DELIVERY') ? 'delivery_type' : ''%>" href="javascript:void(0)" id='payment-tab'>Payment</a></li>
                  <li><a class="<%= @pos_check.order_type&.name&.titleize&.upcase ? '' : 'active' %> menu-list <%= @pos_check.check_status.eql?('closed') ? 'reopen_check' : '' %>" href="javascript:void(0)" id="function_reports_link">Function/Report</a></li>
                  <li><a class="menu-list <%= @pos_check.check_status.eql?('closed') ? 'reopen_check' : '' %>" href="javascript:void(0)" id="additional_item">Additional</a></li>
                  <li style="float: right;height: 10px;margin-left: 10px;" class="newOrderFoodClub">
                     <span><h3 style="cursor: pointer;color: white;padding: 17px;margin: auto;" class="orderCountBtn <%= @newOrders.to_i == 0 ? 'green-border' : 'red-border' %>"><b>FOOD CLUB ORDER </b><span class="fcSpan fcSpanDesign"><%= @newOrders.to_i %></span></h3></span>
                  </li>
                  <li style="float: right;border: 1px solid blue;background-color: blue;">
                    <%= link_to 'QuickPay', business_pos_quick_pay_popup_path(restaurant_id: encode_token(@pos_check&.branch&.restaurant_id)), remote: true, class: "menu-list quick_pay_btn #{@pos_check.check_status.eql?('closed') ? 'reopen_check' : ''}", style: "font-weight: bolder;color: white;"  %>
                  </li>
               </ul>
            </div>
          </div>
          <div class="row">
            <div class="col-sm-4">
               <div class="well">
                  <div class="twell">
                     <div class="row" style="height: 225px;overflow-y: auto;">
                        <div class="table-slip">
                           <div class="col-sm-4 displayCheckNo <%= 'hide' unless @pos_check.order_type&.name&.titleize&.upcase %>" style="padding-right: 0px;">
                              CHK NO <%= @pos_check&.check_id %>
                           </div>
                           <div class="col-sm-4 displayCheckType">
                              <%= @pos_check.present? ? @pos_check.order_type&.name&.titleize&.upcase : 'DINE IN' %> 
                           </div>
                           <div class="col-sm-4 displayTbl <%= 'hide' if @pos_check.check_status == 'closed' %>" style="text-align: right;">
                              <% if @pos_check.pos_table.present? && @pos_check.order_type_id.eql?(1) %>
                                <% pos_count = tableCount(@pos_table, @pos_check) %>
                                TBL <%= @pos_table&.pos_table_no %><%= pos_count ? "/#{pos_count}" : ''%>
                              <% end %>
                           </div>
                        </div>
                        <div class="table-slip">
                          <div class="col-sm-6" style="display: block;font-weight: lighter;">
                            <input type='hidden' value="<%= @pos_check.check_status %>" id="check_status">
                            <input type='hidden' value="<%= @pos_check.user_id %>" id="check_customer_id">
                            <input type='hidden' value="<%= @pos_check.driver_id %>" id="check_driver_id">
                            <input type='hidden' value="<%= @pos_check.order_type_id %>" id="check_order_type_id">
                              <span class="customer-details">
                                <% if @pos_check.address_id.present? %>
                                  <span><%= @pos_check.user.name %></span></br>
                                  <span><%= @pos_check.address.area_name %></span>
                                <% end %>
                              </span>
                          </div>
                          <div class="col-sm-6 mt-1 mb-1" style="float: right;text-align: right;padding-right: 10px;">
                                <span class="totalGuestSpan <%= @pos_check.no_of_guest.present? ? '' : 'hide' %>">TOTAL GUEST: <span class="displayNoofGuest"><%= @pos_check.present? ? @pos_check&.no_of_guest : @pos_table.no_of_guest ? check_guest_type(@pos_table.no_of_guest) : 0 %></span>
                                </span>
                          </div>
                        </div>
                        <table>
                          <thead>
                            <tr>
                              <th style='width: 10%'>Qty</th>
                              <th style='width: 55%'>Name</th>
                              <th style='width: 20%'>Price</th>
                              <th style='width: 15%'>Seat</th>
                            </tr>
                          </thead>
                          <tbody id='pos_transactions_list'>
                            <% @transactions.includes(:addon_pos_transactions).each do |transaction| %>
                              <%= render partial: 'transaction_data_list', locals: {transaction: transaction} %>
                            <% end %>
                          </tbody>
                        </table>
                     </div>
                  </div>
                  <div class="payment_amount_details">
                    <% if @pos_check.present? && @pos_check.pos_payments.present? %>
                      <% @pos_check.pos_payments.each do |payment| %>
                        <% if payment.payment_method_id != 4 %>
                          <div style='margin: 10px 0px;' class='payment_detail' data-payment-id="<%= payment&.id %>">
                            <span><%= payment.payment_method.name %></span>
                            <span style="float: right;"><%= number_with_precision(payment.paid_amount, precision: 3) %><%= payment.currency_type.present? ? payment.currency_type.currency : 'BHD' %></span>
                          </div>
                        <% end %>
                      <% end %>
                    <% end %>
                  </div>
                  <input type="hidden" value="<%= @tax %>" id="tax_percentage">
                  Sub Total 
                  <div class="pull-right" id='pos_sub_total'><%= before_tax_amount(@tax, @all_transactions.pluck(:total_amount).sum) %>BHD</div>
                  <br>
                  <br>
                  <% tax_amount = taxonlyamount(@tax, @all_transactions.pluck(:total_amount).sum)%>
                  <% offer_text = @pos_check.offer.present? ? @pos_check.offer.discount_percentage.to_f == 0.0 ? @pos_check.offer.offer_title : @pos_check.offer.discount_percentage.to_s + "%"  : nil %>
                  <% discount_per = @pos_check.offer.present? ? @pos_check.offer.discount_percentage.to_f == 0.0 && @pos_check.offer.offer_title.eql?('Buy 1 Get 1 Free') ? 50 : @pos_check.offer.discount_percentage.to_f : 0.0 %>
                  <% if @pos_check.offer.present? && @pos_check.offer.menu_item_id.present? %>
                    <% addons = ItemAddon.where(item_addon_category_id: @pos_check.offer.menu_item.item_addon_categories.pluck(:id))  %>
                    <% total_ind_amount = @pos_check.pos_transactions.where(
                    '(itemable_type = ? && itemable_id = ?) || (itemable_type = ? && itemable_id IN (?))', 
                    "MenuItem", @pos_check&.offer&.menu_item_id, "ItemAddon", addons.pluck(:id) )&.pluck(:total_amount)&.sum.to_f %>
                  <% else %>
                    <% total_ind_amount = 0.0 %>
                  <% end %>
                  <% transaction_total = @all_transactions.pluck(:total_amount).sum.to_f %>
                  <% individual_tax = @pos_check&.offer&.offer_type.eql?('individual')  ? taxonlyamount(@tax, total_ind_amount) : 0.0 %>
                  <% discount = 
                    @pos_check.offer_id.present? ? @pos_check.offer.offer_type.eql?('individual') ? ((@pos_check.is_full_discount ? total_ind_amount : (total_ind_amount - individual_tax.to_f))  * discount_per / 100) : @all_transactions.length > 0 ? ((@pos_check.is_full_discount ? transaction_total : (transaction_total - tax_amount.to_f)) * (discount_per) / 100) : 0 : 0%>
                  <div class="gift-card-amount">
                    <% if @pos_check.present? && @pos_check.pos_payments.present? && @pos_check.pos_payments.pluck(:payment_method_id).include?(4)  %>
                      <% payment = @pos_check.pos_payments.find_by payment_method_id: 4 %>
                      <div style='margin: 0px 0px 15px 0px;' class='payment_detail' data-payment-id="<%= payment&.id %>">
                        <span><%= payment.payment_method.name %></span>
                        <span style="float: right;"><%= number_with_precision(payment.paid_amount, precision: 3) %><%= payment.currency_type.present? ? payment.currency_type.currency : 'BHD' %></span>
                      </div>
                    <% end %>
                  </div>
                  <div class='<%= @pos_check.offer_id.present? ? '' : 'hide' %>' id='discount_row' data-pos-check-id='<%= @pos_check.id %>'>
                    <div class="hide" id="discountPer"><%= discount_per %></div>
                    <div class="pull-left"> Discount<span id="discountPerText">(<%= offer_text %>)</span></div>
                    <div class="pull-right" id="discount_amount"><%= number_with_precision(discount, precision: 3) %>BHD</div>
                    <br><br>
                  </div>
                  Tax ( <%= @tax_name.join(", ") %> <%= @tax.to_i == @tax ? @tax.to_i : @tax %>% )
                  <div class="pull-right" id="tax_amount"><%= number_with_precision(tax_amount, precision: 3) %>BHD</div>
                  <br><br>
                  Total Due
                  <div class="pull-right" id='pos_total'><%= number_with_precision((@all_transactions.pluck(:total_amount).sum - @pos_check.pos_payments.pluck(:paid_amount).sum - discount), precision: 3) %>BHD</div>
                </div>
                <%= link_to 'Save Check', 'javascript:void(0)', 'data-save_check_link': business_save_check_path(params[:restaurant_id], 'no_table', @pos_check&.id, {is_save_check_btn: true}), method: :post, remote: true, class: "btn btn-primary noMenu #{@pos_check.present? && !@pos_check.pos_transactions.present? && @pos_check.order_type&.name&.titleize&.upcase ? 'disabled' : ''}", id: "pos_check_save" %>
                <%= link_to 'Schedule', 'javascript:void(0)', class: "btn btn-primary schedule_categoring #{@pos_check&.order_type&.name.eql?('Catering') ? '' : 'hide'}", id: "schedule_categoring" %>
                <%= link_to 'Print Check', business_print_check_path(@pos_table&.id, @pos_check&.id), method: :post, remote: true,  class: "btn btn-primary noMenu #{@pos_check.present? && !@pos_check.pos_transactions.present?  && @pos_check.order_type&.name&.titleize&.upcase ? 'disabled' : ''}", style: "float: right;" %>
            </div>
            <div class="col-sm-1" style="margin-left: -2%;">
               <div class="well" style="padding: 10px">
                <%#= hidden_field_tag 'selected_menu_item' %>
                <%= hidden_field_tag 'selected_qty' %>
                <% total_amount = @all_transactions.pluck(:total_amount).sum - @pos_check.pos_payments.pluck(:amount).sum %>
                <%= hidden_field_tag 'remain_payment_amount', number_with_precision(total_amount, precision: 3), id: 'remain_payment_amount' %>
                <%= hidden_field_tag 'pos_check_id', @pos_check&.id, id: 'pos_check_id' %>
                <%= hidden_field_tag 'restaurant_id', params[:restaurant_id], id: 'pos_restaurant_id' %>
                <%= hidden_field_tag 'table_id', params[:table], id: 'pos_table_id' %>
                  <% (0..9).each do |i| %>
                    <%= link_to "javascript:void(0)", 'data-selected-qty' => i, class: 'qty_selection' do %>
                      <div class="blue">
                        <%= i %>
                      </div>
                    <% end %>
                  <% end %>
                  <%= link_to "javascript:void(0)", 'data-selected-qty' => '.', class: 'qty_selection' do %>
                    <div class="blue">
                      .
                    </div>
                  <% end %>
               </div>
            </div>
            <div class="food_listing <%= ((@pos_check.present? && @pos_check.order_type_id.eql?(2) && @pos_check.pos_transactions.present? && @pos_check.check_status == "saved") || @pos_check.check_status == "closed") ? 'disabled' : '' %> <%= @pos_check.order_type&.name&.titleize&.upcase ? '' : 'hide' %> " id='food_listing'>
                <div class="col-sm-6" style="margin-left: -2%;">
                   <div class="cata">
                      <span class="menu-category-text">MENU CATEGORIES</span>
                      <%= link_to 'Back', business_partner_pos_menu_categories_path(params[:restaurant_id], pos_check_id: @pos_check.id), class: 'btn btn-default back-menu-item hide',style: 'float: left', remote: true %>
                   </div>
                </div>
                 <div class="col-sm-6" style="margin-left: -2%;">
                    <div class="menu" style="height: 472px;overflow-y: auto;">
                      <div class="row pos-transaction" style="margin: auto;margin-left: 2%;">
                        <% @categories.each do |category| %>
                          <%= link_to business_partner_pos_menu_items_path(category, pos_table_id: @pos_table&.id), remote: true do  %>
                            <div class="card" style="display: inline-block;
                              height: 60px;
                              width: 32%;
                              padding-bottom: 15px;
                              border-radius: 5px;
                              text-align: center;
                              vertical-align: middle;">
                                  <div class="card-body">
                                    <p style="color: black;top: 15px;position: relative;text-align: center;"><%= category.category_title %>
                                    </p>
                                  </div>
                            </div>
                          <% end %>
                        <% end %>
                      </div>
                      <!-- <div class="row pos-addon-items hide" style="margin: auto;margin-left: 2%;">
                      </div>

                      <div class="row comment-form hide" style="margin: auto;margin-left: 2%;">
                        <%#= form_for :branch, url: business_pos_save_comment_path, method: :post, html: { class: "comments-form", remote: true, autocomplete: :off } do |f| %>
                          <%#= hidden_field_tag(:table_id, params[:table]) %>
                          <%#= hidden_field_tag(:selected_menu_item_id) %>
                          <%#= hidden_field_tag(:selected_menuaddon_transaction_id) %>
                          <%#= hidden_field_tag(:selected_menuaddon_id) %>
                          <%#= hidden_field_tag(:addon_item_count) %>

                          <div style="padding: 20px 0px 0px 0px;">
                                  <%#= label_tag :comments, "Comments" %><br>
                                  <%#= text_area_tag(:comments, nil,  rows: 5, cols: 50) %>
                                  <%#= submit_tag "Add Comment", class: "btn btn-success pull-right", style: 'display: block;position: absolute;margin-bottom: 13px;' %>
                          </div>
                        <%# end %>
                      </div> -->
                   </div>
                </div>
                <div class="col-sm-1" style="padding: 0px;width: 11%;margin-top: -2.4%">
                  <div class="well first-letter-list" style="height: 500px;overflow-y: scroll;">
                      <div class="blue">ALL</div>
                      <% @categories.map { |cat| cat.category_title[0].titleize }.uniq.sort.each do |category_name| %>
                        <div class="blue"><%= category_name %></div>
                      <% end %>
                  </div>

                  <!-- <div class="well addon-categories hide">
                  </div> -->
                </div>
            </div>

            <div class="pos_payment hide" id='pos_payment'>
                <div class="col-sm-6 pos_payment_data paymentMethodList" style="margin-left: -2%;">
                    <div class="" style="height: 472px;overflow-y: auto;">
                        <div class="row" style="margin: auto;margin-left: 2%;">
                            <% PaymentMethod.all.each do |payment_method| %>
                                <% if payment_method.id != 4 %>
                                  <div class="col-md-4 box functionListingDiv paymentDiv" data-payment-method-id="<%= payment_method.id %>">
                                      <span class="functionListingSpan"><%= payment_method.name %></span>
                                  </div>
                                <% end %>
                            <% end %>
                        </div>
                   </div>
                </div>
                <div class="col-sm-6 pos_payment_data discountList hide" style="margin-left: -2%;">
                    <% payment_method_4 = PaymentMethod.find_by_id(4) %>
                    <div class="" style="height: 472px;overflow-y: auto;">
                      <div class="row" style="margin: auto;margin-left: 2%;">
                        <a href='javascript:void(0)' class="paymentDiv" data-payment-method-id="<%= payment_method_4.try(:id) %>" data-remote=true><button class='discount_img gift_coupon_btn <%= @pos_check.pos_payments.where(pending_delete: false).pluck(:payment_method_id).include?(4) ? 'selected_discount' : '' %>'><%= payment_method_4.try(:name) %></br><span>&nbsp;</span></button></a>
                        <span class="discountData"></span>
                      </div>
                    </div>
                </div>
                <div class="col-sm-1" style="padding: 0px;width: 11%;">
                  <div class="well" style="height: auto;overflow-y: scroll;">
                      <div class="blue payment-option active-menuon foodPayment paybtn">Pay</div>
                      <%= link_to business_get_discount_percentage_path(branch_id: @branch.id, pos_check_id: @pos_check&.id), remote: true do %>
                        <div class="blue payment-option">
                          Discount
                        </div>
                      <%end%>
                      <div class="blue payment-option hide">Foreign Currency</div>
                  </div>
                </div>
            </div>

            <div class="function_listing <%= @pos_check.order_type&.name&.titleize&.upcase ? 'hide' : '' %>" id='function_listing'>
                <div class="col-sm-6 all-functions" style="margin-left: -2%;">
                    <div class="" style="height: 472px;overflow-y: auto;">
                        <div class="row" style="margin: auto;margin-left: 2%;">
                            <div class="col-md-4 box functionListingDiv coverNavigation">
                                <span class="functionListingSpan">Change Cover</span>
                            </div>

                            <div class="col-md-4 box functionListingDiv bindCustomerWithCheck">
                                <span class="functionListingSpan">Check ID</span>
                            </div>

                            <div class="col-md-4 box functionListingDiv">
                                <span class="functionListingSpan">EMP ID</span>
                            </div>

                            <div class="col-md-4 box functionListingDiv validated_field <%= ['reopened_pending' , 'pending'].include?(@pos_check.check_status) && @pos_check.order_type_id.present? ? 'pending_transaction' : '' %> delivery_restriction <%= @pos_check.order_type&.name&.titleize&.upcase.eql?('DELIVERY') ? 'delivery_type' : ''%>" id="reopen_check">
                                <span class="functionListingSpan">Reopen Closed Check</span>
                            </div>

                            <div class="col-md-4 box functionListingDiv validated_field <%= ['reopened_pending' , 'pending'].include?(@pos_check.check_status) && @pos_check.order_type_id.present? ? 'pending_transaction' : '' %> delivery_restriction <%= @pos_check.order_type&.name&.titleize&.upcase.eql?('DELIVERY') ? 'delivery_type' : ''%>" id="edit_closed_check">
                                <span class="functionListingSpan">Edit Closed Check</span>
                            </div>

                            <div class="col-md-4 box functionListingDiv">
                                <span class="functionListingSpan">Reprint Closed Check</span>
                            </div>

                            <div class="col-md-4 box functionListingDiv transferTableDiv">
                                <span class="functionListingSpan">Transfer Table</span>
                            </div>

                            <div class="col-md-4 box functionListingDiv">
                              <%#= link_to 'javascript:void(0)', 'data-toggle': "modal",  'data-target': "#{
                                @pos_check.check_type.eql?('dine_in') ? '' : '#transferCheckData'}",  class: "functionListingSpan #{
                                  @pos_check.check_type.eql?('dine_in') ? 'DineInError' : '' }" do %>
                              <%= link_to 'javascript:void(0)', 'data-toggle': "modal",  'data-target': "#transferCheckData",  class: "functionListingSpan" do %>
                                <span class="">Transfer Chk Number</span>
                              <% end %>
                            </div>

                            <div class="col-md-4 box functionListingDiv">
                                <span class="functionListingSpan">Return</span>
                            </div>

                            <div class="col-md-4 box functionListingDiv validated_field <%= ['reopened_pending' , 'pending'].include?(@pos_check.check_status) ? 'pending_transaction' : '' %> splitCheckDiv" data-check-id="<%= @pos_check&.id %>">
                              <span class="functionListingSpan">Split Check</span>
                            </div>

                            <div class="col-md-4 box functionListingDiv">
                                <span class="functionListingSpan">Change RVC</span>
                            </div>

                            <div class="col-md-4 box functionListingDiv">
                                <span class="functionListingSpan">No Sale</span>
                            </div>

                            <% OrderType.where(is_enabled: true).each do |order_type| %>
                                <div class="col-md-4 box functionListingDiv posCheckStatus" data-status="<%= order_type.id %>">
                                    <span class="functionListingSpan"><%= order_type.name %></span>
                                </div>
                            <% end %>
                        </div>
                   </div>
                </div>

                <div class="col-sm-6 hide all-reports" style="margin-left: -2%;">
                    <div class="" style="background-color: white;height: 472px;overflow-y: auto;">
                      <div class="row" style="margin: auto;margin-left: 2%;">
                        <table>
                          <tr>
                            <th>File Name</th>
                            <th>Download</th>
                          </tr>
                          <tbody class="report-list">
                            <% if @pos_check.present? && @pos_check.pos_payments.present? && @pos_check.pos_payments.select { |a| a.attachments.present? }.present? %>
                              <% @pos_check.pos_payments.map { |a| a.attachments }.flatten.uniq.compact.each do |attachment_data| %>
                                <tr>
                                  <td><%= attachment_data.blob.filename %></td>
                                  <td><%= link_to 'View', url_for(attachment_data), target: '_blank', style: 'color: blue;' %></td>
                                </tr>
                              <% end %>
                            <% else %>
                              <tr>
                                <td colspan="2" style="text-align: center;">Data not found</td>
                              </tr>
                            <% end %>
                          </tbody>
                        </table>
                      </div>  
                   </div>
                </div>
                <div class="col-sm-1" style="padding: 0px;width: 11%;">
                  <div class="well" style="height: auto;overflow-y: scroll;">
                      <div class="blue tab-function-and-reports functions-tab active-menuon">Functions</div>
                      <div class="blue tab-function-and-reports report-tab">Reports</div>
                      <div class="blue hide">PMC</div>
                  </div>
                </div>
            </div>
            <div class='col-md-7'>
              <div class="ml-0 hide" id='condiments_data'>
                <div class='col-md-10 condiment-header'>
                  <p class="comment-text">Comments</p>
                </div>
                <div class='col-md-2 text-right' style="padding: 0 11px 0 1px; float: right;">
                  <button class='btn mr-2 condiments-btn active' id='con_comment'>Condiment</button>
                  <button class='btn mr-2 mt-1 condiments-btn' id='con_kitchen'>Kitchen <br/>Instruction</button>
                </div>
                <div class='condiment_comment'>
                  <div class="col-md-4" style="padding: 0px;padding-top: 10px;float: left;">
                    <textarea class="item_comment comment_list"></textarea>
                    <%= link_to "Comment", 'javascript:void(0)', class: 'btn btn-primary comment_submit', style: "background-color: #e9f0f8; color: black;" %>
                    <!-- <button class='comment_list'>comments**</button> -->
                  </div>
                    <!-- <div class="col-md-6" style="padding: 0;float: right;">
                    <button class='btn btn-primary menu-btn' style="background-color: #e9f0f8;color: black;">Comments</button>
                  </div> -->
                </div>
                <div class='kitchen_list hide col-md-10'>
