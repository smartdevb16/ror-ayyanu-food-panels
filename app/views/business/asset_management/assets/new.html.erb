<style type="text/css">
  .bg-white {
    background-color: white;
  }

  .form-control {
    margin-bottom: 10px !important;
    height: 40px;
  }

  .setheight {
    height:40px !important;
  }

  .branch-select-div {
    height: 70px;
  }

  .show-detail-back-btn{
    margin-right: 1%;
    margin-top: 1.5%;
    padding-bottom: %;
    padding: 8px 65px 8px;
    outline: none !important;
  }
</style>

<link rel="stylesheet" href="/assets/intlTelInput.css">

<div class="row wrapper border-bottom white-bg page-heading">
	<div class="col-sm-8">
		<h2>Asset Management >> Add Asset >> New</h2>
	</div>

	<div class="col-sm-4">
		<span class="pull-right">
			<h2><%= link_to 'Back <i class="fa fa-arrow-circle-right"></i>'.html_safe, business_asset_management_assets_path(restaurant_id: params[:restaurant_id]), class: "btn btn-danger" %></h2>
		</span>
	</div>
</div>
<br>

<div class="container">
	<div class="panel panel-primary">
		<div class="panel-heading">
			<h3 class="text-center">Add New Asset </h3>
		</div>

		<div class="panel-body">
			<%= render 'form' %>
		</div>
	</div>
</div>

<script type="text/javascript">
	<% if flash[:success].present? %>
	swal("Success", "<%= flash[:success] %>", "success")
	<% elsif flash[:error].present? %>
	swal("Error", "<%= flash[:error] %>", "error")
	<% end %>
</script>
